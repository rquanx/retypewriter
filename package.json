{
  "name": "@retypewriter/monorepo",
  "private": true,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "vite",
    "stub": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.18.8",
    "@antfu/ni": "^0.13.2",
    "@babel/types": "^7.17.0",
    "@types/diff-match-patch": "^1.0.32",
    "@types/node": "^17.0.21",
    "bumpp": "^7.1.1",
    "diff-match-patch": "^1.0.5",
    "eslint": "^8.11.0",
    "esno": "^0.14.1",
    "pnpm": "^6.32.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.0",
    "vite": "^2.8.6",
    "vitest": "^0.7.4"
  }
}
